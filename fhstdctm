#include <stdio.h>
int main()
{
	printf("печать таблицы температур по Фаренгейту и Цельсию для fahr = 0, 20, ..., 300\n");
	float a, b;
	int c, d, e;
	c = 0; /* нижняя граница таблицы температур */
	d = 300; /* верхняя граница */
	e = 20; /* шаг */
	a = c;
	while (a <= d) {
		b = (9*a)/5;
		printf("%f\t%f\n", a, b);
		a = a + e;
	}
}
